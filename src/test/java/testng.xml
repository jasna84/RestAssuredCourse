<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name = "All tests">
    <listeners>
        <listener class-name="main.java.base.ExtentReporterNG"/>
    </listeners>
    <test name = "Health Check Test">
        <classes>
            <class name = "pingTests.HealthCheck"/>
        </classes>
    </test>
    <test name = "Create Token Test">
        <classes>
            <class name = "authTests.CreateToken"/>
        </classes>
    </test>
    <test name = "Create Booking Test">
        <classes>
            <class name = "postTests.CreateBooking"/>
        </classes>
    </test>
    <test name = "Get Booking Test">
        <classes>
            <class name = "getTests.GetBooking"/>
            <class name = "getTests.GetBookingIds"/>
        </classes>
    </test>
    <test name = "Update Booking Test">
        <classes>
            <class name = "putTests.UpdateBooking"/>
            <class name = "putTests.PartialUpdateBooking"/>
        </classes>
    </test>
    <test name = "Delete Booking Test">
        <classes>
            <class name = "deleteTests.DeleteBooking"/>
        </classes>
    </test>
</suite>